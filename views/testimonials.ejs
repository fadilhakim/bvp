<% include template/header %>
    <section class="page-header testimonials">
        <div class="title">
            <h1>Success Stories</h1>
            <div class="border-bottom-text"><div/>
        </div>
    </section>

    <section class="sucess-stories pd-section">
        <div class="container">
            <div class="row">
                <% testimonials.forEach((testimoni)=>{ %>
                <div class="col-lg-6 col-md-6 col-xs-12 testimonial">
                    <div class="row">
                        <div class="col-lg-3 col-xs-12">
                            <div class="profpic">
                                <% if(testimoni.vendor_pic){%>
                                    <img class="img-responsive" src="<%- assetsUrl -%>/vendor_prof_pic/<%= testimoni.vendor_pic %>">
                                <% } else {%>
                                    <img class="no-pic" src="<%- assetsUrl -%>/vendor_prof_pic/user_icon.png">
                                <% } %>
                            </div>
                        </div>
                        <div class="col-lg-9 col-xs-12 text">
                            <p><%= testimoni.vendor_testi %></p>
                            <span class="border-pink"></span>
                            <p class="name f-w700 m-top40"><%= testimoni.vendor_cp %></p>
                            <p class="role"><%= testimoni.vendor_name %></p>
                        </div>
                        <div></div>
                    </div>
                </div>
                <% }) %>
            
            </div>
        </div>
    </section>
<% include template/footer %>