    <section class="vendors pd-section vendor-origin">
        <h2 class="head-section">Vendors Who Have Joined Us </h2>
        <span class="border-pink center m-top20 m-btm20"></span>
        <div class="container">
            <div class="row">
                <% dataVendors.forEach((vendor)=>{ %>
                <div class="col-md-15 col-lg-15">
                        <img class="img-responsive" src="assets/vendor_logo/sg/<%= vendor.vendor_img %>">
                </div>
                <%}) %>
            </div>
        </div>
    </section>

    <section class="text-center m-top40 trial">
        <div class="container">
            <h2 class="head-section white">Want to grow your business with us? Create your FREE business profile now!</h2>
            <a href="#" class="btn btn-primary mid-button center-block m-top30">Start Free Trial</a>
        </div>
    </section>

    <section class="text-center pd-section contact-assist">
        <h2 class="head-section">Need assistance? Contact us today!</h2>
        <span class="border-pink center m-top20 m-btm20"></span>
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-xs-12">
                    <div class="ct-icon">
                        <img class="img-responsive" src="assets/icons/Landing_page_icon-Phone.png" alt="Contact Call">
