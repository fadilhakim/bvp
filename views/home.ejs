    <section class="blogs pd-section">
        <h2 class="head-section">Business Blog</h2>
        <span class="border-pink center m-top20 m-btm20"></span>
        <h3 class="f-w400 m-top30 m-btm30">Resources you need to grow your wedding business.</h3>
        <div class="container">
            <div class="row">

                <%var i = 0; dataBlogs.forEach((blogs)=>{ %>
                <div class="col-lg-4 col-md-12 col-xs-12">
                    <div class="img-blog">
                        <img class="img-responsive" src="<%= blogs.homestreamImage %>">
                    </div>
                    <span class="catagory">
                        <b><%= blogs.categories.data[0].name %></b>
                        - <% 
                            var fd = blogs.createdAt
                            var date = fd.slice(0,10)
                            var newDate = new Date(date) %>
                          <%= newDate.toDateString() %>
                    </span>
                    <h4 class="f-w700">
                            <%= blogs.title %>
                    </h4>
                </div>
                <%}) %>
            </div>
        </div>

        <div class="container text-center m-top40">
            <a href="https://www.bridestory.com/blog" class="btn btn-primary mid-button center-block ">View More Articles</a>
        </div>
